{
  "name": "helloworld",
  "version": "1.0.0",
  "description": "Sample app for ACR Build",
  "main": "server.js",
  "scripts": {
    "start:mac": "export NODE_ENV=development && nodemon --watch src/ --exec \"ts-node src/app.ts\"  -e ts --ignore tests",
    "start": "set NODE_ENV=development && nodemon --watch src/ --exec \"ts-node src/app.ts\"  -e ts --ignore tests",
    "test": "set NODE_ENV=test && jest",
    "prod": "export NODE_ENV=production && node build/app.js",
    "build": "rimraf ./build && tsc"
  },
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.16",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mysql2": "^2.3.3",
    "rimraf": "^4.1.2",
    "typeorm": "^0.3.11",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  }
}